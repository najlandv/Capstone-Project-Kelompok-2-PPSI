<%- include("head.ejs")%>
<main id="content" class="bg-gray-100 py-8">
  <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
    <h2 class="text-2xl mb-6">Notifikasi</h2>
    <% notifications.forEach((notification) => { %>
      <div class="bg-white shadow overflow-hidden sm:rounded-lg mb-4">
        <div class="px-4 py-5 sm:px-6">
          <h3 class="text-lg leading-6 font-medium text-gray-900"><%= notification.title %></h3>
          <p class="mt-1 max-w-2xl text-sm text-black"><%= notification.message %></p>
          <p class="mt-2 text-sm text-black"><%= new Date(notification.createdAt).toLocaleString() %></p>
        </div>
      </div>
    <% }); %>
  </div>
</main> 
<%- include("footer.ejs")%>